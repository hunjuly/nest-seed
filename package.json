{
    "name": "nest-seed",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "nest start --debug 9230 --watch",
        "start:prod": "node dist/app/main",
        "test": "rm -rf dist && node --inspect=9231 -r ts-node/register ./node_modules/.bin/jest --watch --onlyChanged",
        "test:all": "jest --coverage",
        "test:app": "jest --config ./jest.config.app.ts",
        "test:common": "jest --config ./jest.config.common.ts",
        "build": "nest build",
        "lint": "eslint src/**/*.ts --fix",
        "format": "prettier --write src/**/*.ts",
        "migration:generate": "npm run build && typeorm migration:generate -d ./dist/databases/psql/data-source.js ./src/databases/psql/migrations/$npm_package_name",
        "migration:show": "typeorm migration:show -d ./dist/databases/psql/data-source.js",
        "migration:run": "typeorm migration:run -d ./dist/databases/psql/data-source.js",
        "migration:revert": "typeorm migration:revert -d ./dist/databases/psql/data-source.js"
    },
    "dependencies": {
        "@nestjs/cache-manager": "^2.0.0",
        "@nestjs/config": "^3.0.0",
        "@nestjs/core": "^10.0.2",
        "@nestjs/jwt": "^10.1.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/mongoose": "^10.0.2",
        "@nestjs/passport": "^10.0.0",
        "@nestjs/platform-express": "^10.0.2",
        "@nestjs/typeorm": "^10.0.0",
        "bcrypt": "^5.1.0",
        "cache-manager": "^5.2.3",
        "cache-manager-ioredis-yet": "^1.2.2",
        "cache-manager-redis-store": "^3.0.1",
        "chalk": "^4.1.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "ioredis": "^5.3.2",
        "lodash": "^4.17.21",
        "mongoose": "^8.0.3",
        "npm-check-updates": "^16.10.12",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.11.0",
        "redis": "^4.6.7",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "typeorm": "^0.3.17",
        "winston": "^3.9.0",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.3",
        "@nestjs/schematics": "^10.0.1",
        "@nestjs/testing": "^10.0.2",
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.17",
        "@types/jest": "29.5.2",
        "@types/lodash": "^4.14.195",
        "@types/node": "20.3.1",
        "@types/passport-jwt": "^3.0.8",
        "@types/passport-local": "^1.0.35",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "@typescript-eslint/parser": "^5.60.0",
        "eslint": "^8.43.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "29.5.0",
        "mongodb-memory-server": "^9.1.3",
        "prettier": "^2.8.8",
        "source-map-support": "^0.5.21",
        "sqlite3": "^5.1.6",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "<5.1.0"
    }
}
