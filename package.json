{
    "name": "nest-seed",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "nest start --debug 9230 --watch",
        "start:prod": "node dist/app/main",
        "test": "rm -rf dist && node --inspect=9231 -r ts-node/register ./node_modules/.bin/jest --watch --onlyChanged",
        "test:all": "IGNORE_LOGGING_DURING_TESTING=true jest --coverage",
        "test:app": "IGNORE_LOGGING_DURING_TESTING=true jest --config ./jest.config.app.ts",
        "test:common": "IGNORE_LOGGING_DURING_TESTING=true jest --config ./jest.config.common.ts",
        "build": "nest build",
        "lint": "eslint src/**/*.ts --fix",
        "format": "prettier --write src/**/*.ts",
        "migration:generate": "npm run build && typeorm migration:generate -d ./dist/databases/psql/data-source.js ./src/databases/psql/migrations/$npm_package_name",
        "migration:show": "typeorm migration:show -d ./dist/databases/psql/data-source.js",
        "migration:run": "typeorm migration:run -d ./dist/databases/psql/data-source.js",
        "migration:revert": "typeorm migration:revert -d ./dist/databases/psql/data-source.js"
    },
    "dependencies": {
        "@nestjs/bull": "^10.1.1",
        "@nestjs/cache-manager": "^2.2.2",
        "@nestjs/core": "^10.3.10",
        "@nestjs/event-emitter": "^2.0.4",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/mapped-types": "^2.0.5",
        "@nestjs/mongoose": "^10.0.10",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.3.10",
        "@nestjs/typeorm": "^10.0.2",
        "bcrypt": "^5.1.1",
        "bull": "^4.15.1",
        "cache-manager": "^5.7.1",
        "cache-manager-ioredis-yet": "^2.1.1",
        "chalk": "4.1.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "compression": "^1.7.4",
        "express": "^4.19.2",
        "lodash": "^4.17.21",
        "mongoose": "^8.4.5",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.12.0",
        "rxjs": "^7.8.1",
        "typeorm": "^0.3.20",
        "winston": "^3.13.0",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.4.2",
        "@nestjs/schematics": "^10.1.2",
        "@nestjs/testing": "^10.3.10",
        "@types/bcrypt": "^5.0.2",
        "@types/compression": "^1.7.5",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/lodash": "^4.17.6",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.14.10",
        "@types/passport-jwt": "^4.0.1",
        "@types/passport-local": "^1.0.38",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^7.15.0",
        "@typescript-eslint/parser": "^7.15.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "jest": "^29.7.0",
        "mongodb-memory-server": "^9.4.0",
        "prettier": "^3.3.2",
        "sqlite3": "^5.1.7",
        "supertest": "^7.0.0",
        "ts-jest": "^29.1.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.3"
    }
}
